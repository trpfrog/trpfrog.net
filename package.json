{
  "name": "trpfrog.net",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint-fix": "turbo lint-fix && prettier . --write",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "reinstall-all-deps": "rm -rf node_modules ./*/*/node_modules && pnpm install",
    "prepare": "lefthook install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.6",
    "@evilmartians/lefthook": "^1.9.3",
    "@trpfrog.net/config-eslint": "workspace:*",
    "@types/node": "^20.17.10",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "2.1.5",
    "@vitest/ui": "^2.1.8",
    "better-typescript-lib": "^2.10.0",
    "eslint": "^9.14.0",
    "happy-dom": "^15.11.7",
    "prettier": "3.3.3",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-merge": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tsr": "^1.3.3",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.96.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.2",
      "eslint": "npm:eslint@9.14.0"
    }
  },
  "packageManager": "pnpm@9.1.4"
}
