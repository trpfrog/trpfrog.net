{
  "name": "trpfrog.net",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint-fix": "turbo lint-fix && prettier . --write",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "reinstall-all-deps": "rm -rf node_modules ./*/*/node_modules && pnpm install",
    "prepare": "lefthook install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.1.0",
    "@evilmartians/lefthook": "^1.12.2",
    "@trpfrog.net/config-eslint": "workspace:*",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "better-typescript-lib": "^2.11.0",
    "eslint": "npm:eslint@9.14.0",
    "happy-dom": "^18.0.1",
    "prettier": "3.6.2",
    "prettier-plugin-classnames": "^0.8.2",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsr": "^1.3.4",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.26.1"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.2",
      "eslint": "npm:eslint@9.14.0"
    }
  },
  "packageManager": "pnpm@9.1.4",
  "dependencies": {
    "valibot": "1.1.0"
  }
}
