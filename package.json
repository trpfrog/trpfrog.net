{
  "name": "trpfrog.net",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint-fix": "turbo lint-fix && prettier . --write",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "reinstall-all-deps": "rm -rf node_modules ./*/*/node_modules && pnpm install",
    "prepare": "lefthook install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.2",
    "@evilmartians/lefthook": "^1.11.13",
    "@trpfrog.net/config-eslint": "workspace:*",
    "@types/node": "^22.15.29",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "3.2.1",
    "@vitest/ui": "^3.2.1",
    "better-typescript-lib": "^2.11.0",
    "eslint": "9.28.0",
    "happy-dom": "^17.6.1",
    "prettier": "3.5.3",
    "prettier-plugin-classnames": "^0.7.8",
    "prettier-plugin-merge": "^0.7.4",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tsr": "^1.3.4",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.1",
    "wrangler": "^4.18.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.2",
      "eslint": "npm:eslint@9.14.0"
    }
  },
  "packageManager": "pnpm@9.1.4",
  "dependencies": {
    "valibot": "1.1.0"
  }
}
